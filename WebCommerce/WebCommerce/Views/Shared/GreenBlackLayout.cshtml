    <div id="menu1">
        <div class="site-header">
            <div class="container">
                <div class="row">
                    <div class="col-md-4 col-sm-6 col-xs-6">
                        <div id="templatemo_logo">
                            <h1><a href="#">@ViewBag.LayoutTitle</a></h1>
                        </div> <!-- /.logo -->
                    </div> <!-- /.col-md-4 -->
                    <div class="col-md-8 col-sm-6 col-xs-6">
                        <a href="#" class="toggle-menu"><i class="fa fa-bars"></i></a>
                        <div class="main-menu">
                            <ul>
                                <li><a href="#menu1">@ViewBag.Menu1</a></li>
                                <li><a href="#menu2">@ViewBag.Menu2</a></li>
                                <li><a href="#menu3">@ViewBag.Menu3</a></li>
                                <li><a href="#menu4">@ViewBag.Menu4</a></li>
                                <li><a href="#menu5">@ViewBag.Menu5</a></li>
                            </ul>
                        </div> <!-- /.main-menu -->
                    </div> <!-- /.col-md-8 -->
                </div> <!-- /.row -->
                <div class="row">
                    <div class="col-md-12">
                        <div class="responsive">
                            <div class="main-menu">
                                <ul>
                                    <li><a href="#menu1">@ViewBag.Menu1</a></li>
                                    <li><a href="#menu2">@ViewBag.Menu2</a></li>
                                    <li><a href="#menu3">@ViewBag.Menu3</a></li>
                                    <li><a href="#menu4">@ViewBag.Menu4</a></li>
                                    <li><a href="#menu5">@ViewBag.Menu5</a></li>
                                </ul>
                            </div>
                        </div>
                    </div>
                </div>
            </div> <!-- /.container -->
        </div> <!-- /.site-header -->
    </div> <!-- /#front -->

    <div class="site-slider">
        <ul class="bxslider">
            <li>
                <img src="~/Images/GreenBlack/slider/slide1.jpg" alt="slider image 1">
                <div class="container">
                    <div class="row">
                        <div class="col-md-12 text-right">
                            <div class="slider-caption">
                                <h2>Your HTML5 Website</h2>
                            </div>
                        </div>
                    </div>
                </div>
            </li>
            <li>

                <img src="~/Images/GreenBlack/slider/slide2.jpg" alt="slider image 2">
                <div class="container caption-wrapper">
                    <div class="slider-caption">
                        <h2>Free Bootstrap Template</h2>
                    </div>
                </div>
            </li>
            <li>
                <img src="~/Images/GreenBlack/slider/slide3.jpg" alt="slider image 3">
                <div class="container">
                    <div class="row">
                        <div class="col-md-12 text-right">
                            <div class="slider-caption">
                                <h2>Mobile Ready Design</h2>
                            </div>
                        </div>
                    </div>
                </div>
            </li>
            <li>
                <img src="~/Images/GreenBlack/slider/slide4.jpg" alt="slider image 4">
                <div class="container">
                    <div class="row">
                        <div class="col-md-12 text-right">
                            <div class="slider-caption">
                                <h2>Responsive Bootstrap</h2>
                            </div>
                        </div>
                    </div>
                </div>
            </li>
            <li>
                <img src="~/Images/GreenBlack/slider/slide5.jpg" alt="slider image 5">
                <div class="container">
                    <div class="row">
                        <div class="col-md-12 text-right">
                            <div class="slider-caption">
                                <h2>Get it for free!</h2>
                            </div>
                        </div>
                    </div>
                </div>
            </li>
        </ul> <!-- /.bxslider -->
        <div class="bx-thumbnail-wrapper">
            <div class="container">
                <div class="row">
                    <div class="col-md-12">
                        <div id="bx-pager">
                            <a data-slide-index="0" href=""><img src="~/Images/GreenBlack/slider/thumb1.jpg" alt="image 1" /></a>
                            <a data-slide-index="1" href=""><img src="~/Images/GreenBlack/slider/thumb2.jpg" alt="image 2" /></a>
                            <a data-slide-index="2" href=""><img src="~/Images/GreenBlack/slider/thumb3.jpg" alt="image 3" /></a>
                            <a data-slide-index="3" href=""><img src="~/Images/GreenBlack/slider/thumb4.jpg" alt="image 4" /></a>
                            <a data-slide-index="4" href=""><img src="~/Images/GreenBlack/slider/thumb5.jpg" alt="image 5" /></a>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div> <!-- /.site-slider -->

    <div id="menu2" class="content-section">

        <div class="row">
            <div class="col-md-12 text-center">
                <h1 class="section-title">@ViewBag.Menu2</h1>
            </div> <!-- /.col-md-12 -->
            @RenderSection("Menu2");
        </div> <!-- /#menu2 -->
    </div> <!-- /.row -->


    <div id="menu3" class="content-section">

        <div class="row">
            <div class="col-md-12 text-center">
                <h1 class="section-title">@ViewBag.Menu3</h1>
            </div> <!-- /.col-md-12 -->
            @RenderSection("Menu3");
        </div> <!-- /#menu2 -->
    </div> <!-- /.row -->


    <div id="menu4" class="content-section">

        <div class="row">
            <div class="col-md-12 text-center">
                <h1 class="section-title">@ViewBag.Menu4</h1>
            </div> <!-- /.col-md-12 -->
            @RenderSection("Menu4");
        </div> <!-- /#menu2 -->
    </div> <!-- /.row -->


    <div id="menu5" class="content-section">

        <div class="row">
            <div class="col-md-12 text-center">
                <h1 class="section-title">@ViewBag.Menu5</h1>
            </div> <!-- /.col-md-12 -->
            @RenderSection("Menu5");
        </div> <!-- /#menu2 -->
    </div> <!-- /.row -->1


    <div class="site-footer">
        <div class="container">
            <div class="row">
                <div class="col-md-8 col-sm-6">
                    <span id="copyright">
                        Copyright &copy; 2014 <a href="#">Company Name</a>
                    </span>

                </div> <!-- /.col-md-6 -->
                <div class="col-md-4 col-sm-6">
                    <ul class="social">
                        <li><a href="#" class="fa fa-facebook"></a></li>
                        <li><a href="#" class="fa fa-twitter"></a></li>
                        <li><a href="#" class="fa fa-instagram"></a></li>
                        <li><a href="#" class="fa fa-linkedin"></a></li>
                        <li><a href="#" class="fa fa-rss"></a></li>
                    </ul>
                </div> <!-- /.col-md-6 -->
            </div> <!-- /.row -->
        </div> <!-- /.container -->
    </div> <!-- /.site-footer -->

    <script src="~/Scripts/GreenBlack/vendor/jquery-1.10.1.min.js"></script>
