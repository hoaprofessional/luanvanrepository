    <script src="~/Scripts/GreenBlack/jquery.easing-1.3.js"></script>
    <script src="~/Scripts/GreenBlack/bootstrap.js"></script>
    <script src="~/Scripts/GreenBlack/plugins.js"></script>
    <script src="~/Scripts/GreenBlack/main.js"></script>
