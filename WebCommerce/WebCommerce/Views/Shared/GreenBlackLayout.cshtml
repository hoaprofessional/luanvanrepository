    <link href="~/Content/GreenBlack/bootstrap.min.css" rel="stylesheet" />
    <link href="~/Content/GreenBlack/normalize.min.css" rel="stylesheet" />
    <link href="~/Content/GreenBlack/font-awesome.min.css" rel="stylesheet" />
    <link href="~/Content/GreenBlack/animate.css" rel="stylesheet" />
    <link href="~/Content/GreenBlack/templatemo_misc.css" rel="stylesheet" />
